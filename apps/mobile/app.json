{
  "name": "打卡助手",
  "slug": "checkin-app",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "userInterfaceStyle": "automatic",
  "scheme": "checkin",
  "splash": {
    "image": "./assets/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "ios": {
    "supportsTablet": true,
    "bundleIdentifier": "com.yourcompany.checkin"
  },
  "android": {
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    },
    "package": "com.yourcompany.checkin"
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/favicon.png"
  },
  "plugins": [
    "expo-router",
    "expo-asset",
    "expo-font"
  ],
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "apiBaseUrl": "http://localhost:3001"
  }
}
